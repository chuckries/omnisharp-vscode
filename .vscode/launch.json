{
    "version": "0.2.0",
    "configurations": [
        {
			"name": "Attach to Extension Host",
			"type": "node",
			"request": "attach",
			"port": 5870,
			"sourceMaps": true,
			"outDir": "out"
		},
        {
            "name": "Launch Extension",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceRoot}"
            ],
            "stopOnEntry": false,
            "sourceMaps": true,
            "outDir": "out"
        }
    ]
}